{
    "compilerOptions": {
        "module": "ES2020",
        "moduleResolution": "node",
        "target": "ES2020",
        "lib": [
            "ES2020",
            "DOM"
        ],
        "allowSyntheticDefaultImports": true,
        "noImplicitAny": true,
        "preserveConstEnums": true,
        "sourceMap": false,
        "outDir": "scripts",
        "skipLibCheck": true,
        //"baseUrl": "./src"
    },
    "include": [
        "src",
        "src/library/*"    
, "src/swss2/mode/.ts"    ],
    "exclude": [
        "node_modules",
        "./node_modules",
        "./node_modules/*",
        "./node_modules/@types/node/index.d.ts",
        "scripts",
    ],
}